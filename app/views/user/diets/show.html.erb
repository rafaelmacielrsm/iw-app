

<div class="spaced-elements content">
  <h3>Diet Details</h3>
  <%= link_to "Back", user_profile_path, 
    class: "waves-effect waves-light btn spaced-elements content" %>
</div>



<div class=content>
  <p>
    <span style="font-weight: 600">Starting Date:</span>
    <%= @diet.starting_date.strftime("%d/%m/%y") %>  
  </p>
  <p>
    <span style="font-weight: 600">Enging Date:</span>
    <%= @diet.ending_date.strftime("%d/%m/%y") %>  
  </p>
  <p>
    <span style="font-weight: 600">Weight Goal:</span>
    <%= "#{@diet.weight_goal / 1000} kg" %>  
  </p>
</div>